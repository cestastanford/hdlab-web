---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title><![CDATA[{{ page.title }} | {{ site.title }}]]></title>
  <link href="{{ site.baseurl }}/{{ page.feed_url }}" rel="self"/>
  <link href="{{ site.baseurl }}/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.baseurl }}/</id>
  <author>
    <name><![CDATA[{{ site.author | strip_html }}]]></name>
    {% if site.email %}<email><![CDATA[{{ site.email }}]]></email>{% endif %}
  </author>
  <generator uri="https://github.com/recurser/jekyll-plugins">Recurser</generator>

  {% for post in site.categories[page.category] limit: 5 %}
  <entry>
    <title type="html"><![CDATA[{{ post.title | cdata_escape }}]]></title>
    <link href="{{ site.baseurl }}{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>{{ site.baseurl }}{{ post.id }}</id>
    <content type="html"><![CDATA[{{ post.content | expand_urls: site.baseurl | markdownify | cdata_escape }}]]></content>
  </entry>
  {% endfor %}
</feed>